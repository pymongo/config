{
    "rust-analyzer.runnableEnv": {},
    // "rust-analyzer.rustcSource": "/home/w/.rustup/toolchains/nightly-x86_64-unknown-linux-gnu/lib/rustlib/rustc-src/rust/compiler/rustc_driver/Cargo.toml",
    // "rust-analyzer.server.path": "/home/w/.rustup/toolchains/nightly-x86_64-unknown-linux-gnu/bin/rust-analyzer",
    // "rust-analyzer.server.path": "/home/w/.config/Code/User/globalStorage/matklad.rust-analyzer/rust-analyzer-x86_64-unknown-linux-gnu",
    "rust-analyzer.server.path": "~/.cargo/bin/rust-analyzer",
    "rust-analyzer.checkOnSave.command": "clippy",
    // "rust-analyzer.experimental.procAttrMacros": true, // fix unresolve macro(proc_macro)
    // "rust-analyzer.procMacro.enable": false,
    "rust-analyzer.checkOnSave.enable": true,
    "rust-analyzer.lens.methodReferences": true,
    // "rust-analyzer.lens.references": true,
    // "rust-analyzer.cargo.loadOutDirsFromCheck": true, // Unknown config

    "terminal.integrated.macOptionIsMeta": true,
    "terminal.integrated.tabs.enabled": true,
    "terminal.integrated.enableBell": true,
    "terminal.integrated.scrollback": 50000,
    "terminal.integrated.tabs.location": "left",
    "files.autoSaveDelay": 1000,
    "editor.suggestSelection": "first",
    "window.openFoldersInNewWindow": "on",
    "security.workspace.trust.untrustedFiles": "open",
    "files.autoSave": "afterDelay",
    "files.exclude": {
        ".idea": true,
        ".gitmodules": true
        // "target": true,
        // "Cargo.lock": true,
    },

    "errorLens.enabledDiagnosticLevels": [
        "error",
        "warning",
        "info"
    ],
    "errorLens.gutterIconsEnabled": true,

    // "go.testFlags": ["-v"],
    // "C_Cpp.clang_format_path": "/usr/bin/clang-format",
    // "vim.insertModeKeyBindings": [{
    //     "before": ["j","j"],
    //     "after": ["<Esc>"]
    // }],

    "cSpell.diagnosticLevel": "Hint",
    "cSpell.userWords": [
        // c or linux
        "syscall",
        "errno",
        "libc",
        "srand",
        "strftime",
        "println",
        "lldb",
        // rust
        "usize",
        "llvm",
        "isahc",
        "rustc",
        "clippy",
        "peekable",
        "lalrpop",
        "chrono",
        "repr",
        "concat",
        "readline",
        "struct",
        // leetcode
        "num",
        "nums",
        "leetcode",
        "lintcode",
        "curr",
        // web
        "openapi",
        "jsonwebtoken",
        "protobuf",
        // business
        // other
        "graphviz",
        "datetime",
        "hasher",
        "manjaro",
        "centos",
        "codeforces",
        "eval",
        "upsert",
        "clickhouse"
    ]
}
